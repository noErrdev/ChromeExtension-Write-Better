{
    "name": "Write Better",
    "version": "0.0.1",
    "description": "English grammar suggestions for better writing",
    "manifest_version": 2,
    "key": "",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "https://docs.google.com/*"
    ],
    "page_action": {
        "default_icon": {
            "16": "images/quill-orange16.png",
            "24": "images/quill-orange24.png",
            "32": "images/quill-orange32.png"
        },
        "default_title": "Write Better",
        "default_popup": ""
    },
    "event_rules": [
        {
            "event": "declarativeContent.onPageChanged",
            "actions": [
                {
                    "type": "declarativeContent.ShowPageAction"
                }
            ],
            "conditions": [
                {
                    "type": "declarativeContent.PageStateMatcher",
                    "pageUrl": {
                        "hostEquals": "docs.google.com",
                        "schemes": [
                            "https"
                        ]
                    }
                }
            ]
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "https://docs.google.com/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ],
    "icons": {
        "16": "images/quill-orange16.png",
        "32": "images/quill-orange32.png",
        "48": "images/quill-orange48.png",
        "128": "images/quill-orange128.png"
    },
    "options_page": "options.html",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}