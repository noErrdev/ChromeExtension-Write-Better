{
    "name": "Write Better",
    "version": "0.1",
    "description": "Grammar suggestions",
    "manifest_version": 2,
    "key": "",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "https://docs.google.com/*"
    ],
    "browser_action": {
        "name": "Click to enable/disable grammar checks",
        "default_icon": "disabled.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://docs.google.com/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ],
    "options_page": "options.html",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}